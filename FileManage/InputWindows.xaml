<Window x:Class="FileManage.InputWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="输入文件夹名" Height="60" Width="300">
    <Window.InputBindings>
        <KeyBinding Command="{x:Static ApplicationCommands.Close}" Key="Enter"/>
    </Window.InputBindings>
    <Grid>
        <TextBox x:Name="AlbumFileName" Width="300" Height="30"/>
    </Grid>
</Window>
